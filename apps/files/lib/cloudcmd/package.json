{
  "name": "cloudcmd",
  "version": "5.3.1",
  "author": "coderaiser <mnemonic.enemy@gmail.com> (https://github.com/coderaiser)",
  "description": "Cloud Commander orthodox web file manager with console and editor",
  "homepage": "http://cloudcmd.io",
  "repository": {
    "type": "git",
    "url": "git://github.com/coderaiser/cloudcmd.git"
  },
  "keywords": [
    "console",
    "editor",
    "file",
    "file manager",
    "folder",
    "orthodox",
    "view",
    "copy",
    "rename",
    "move",
    "delete",
    "file operation"
  ],
  "bin": {
    "cloudcmd": "bin/cloudcmd.js"
  },
  "config": {
    "dirs": "bin lib test"
  },
  "scripts": {
    "start": "node bin/cloudcmd.js",
    "start:dev": "nodemon bin/cloudcmd.js",
    "security": "nsp check",
    "codestyle": "npm-run-all --parallel lint:* jscs spell",
    "lint:css": "recess css/*.css",
    "lint:style": "stylelint css/*.css",
    "lint:js": "jshint $npm_package_config_dirs",
    "jscs": "jscs --esnext $npm_package_config_dirs",
    "test": "tape test/**/*.js",
    "spell": "yaspeller .",
    "wisdom": "npm-run-all --parallel docker:rm docker:rm:alpine ; bin/release.js",
    "postpublish": "npm-run-all --parallel docker docker:alpine",
    "docker:pull:node": "docker pull node",
    "docker:pull:alpine": "docker pull mhart/alpine-node",
    "docker:push": "docker push coderaiser/cloudcmd:`version`",
    "docker:push:alpine": "docker push coderaiser/cloudcmd:`version`-alpine",
    "docker:push:alpine:latest": "docker push coderaiser/cloudcmd:latest-alpine",
    "docker:build": "docker build -t coderaiser/cloudcmd:`version` .",
    "docker:build:alpine": "docker build -f Dockerfile.alpine -t coderaiser/cloudcmd:`version`-alpine .",
    "docker": "npm-run-all docker:pull:node docker:build docker:push",
    "docker:alpine": "npm-run-all docker:pull:alpine docker:build:alpine docker:tag:alpine docker:push:alpine docker:push:alpine:latest",
    "docker:tag:alpine": "docker tag coderaiser/cloudcmd:`version`-alpine coderaiser/cloudcmd:latest-alpine",
    "docker:rm": "docker rmi coderaiser/cloudcmd:`version` || true",
    "docker:rm:alpine": "docker rmi coderaiser/cloudcmd:`version`-alpine || true",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=text-lcov | coveralls"
  },
  "directories": {
    "man": "man"
  },
  "subdomain": "cloudcmd",
  "dependencies": {
    "chalk": "1.1.3",
    "checkup": "1.3.0",
    "copymitter": "1.8.10",
    "criton": "1.0.0",
    "dword": "3.0.16",
    "edward": "3.0.7",
    "execon": "1.2.9",
    "express": "^4.13.4",
    "faust": "1.0.4",
    "files-io": "1.2.6",
    "flop": "1.4.2",
    "format-io": "0.9.6",
    "freeport": "1.0.5",
    "handlebars": "4.7.6",
    "http-auth": "2.2.9",
    "ishtar": "1.3.5",
    "jaguar": "1.1.13",
    "join-io": "1.4.4",
    "jonny": "1.0.1",
    "markdown-it": "6.0.5",
    "mellow": "2.0.2",
    "minify": "2.0.12",
    "minimist": "1.2.0",
    "mollify": "1.0.7",
    "os-homedir": "1.0.2",
    "package-json": "~2.3.0",
    "pipe-io": "1.2.1",
    "ponse": "1.4.2",
    "readjson": "1.1.3",
    "remedy": "1.3.2",
    "rendy": "1.1.0",
    "restafary": "1.6.6",
    "socket.io": "1.4.8",
    "spero": "1.3.2",
    "try-catch": "1.0.0",
    "tryrequire": "1.1.5",
    "writejson": "1.1.1"
  },
  "devDependencies": {
    "coveralls": "^2.11.6",
    "jscs": "^2.7.0",
    "jshint": "^2.8.0",
    "minor": "^1.2.2",
    "morgan": "^1.6.1",
    "nodemon": "^1.9.1",
    "npm-run-all": "^1.4.0",
    "nsp": "^2.2.1",
    "nyc": "^6.0.0",
    "place": "^1.1.4",
    "recess": "^1.1.9",
    "shortdate": "^1.0.1",
    "stylelint": "^5.0.1",
    "stylelint-config-standard": "^4.0.0",
    "tape": "^4.4.0",
    "version-io": "^1.2.5",
    "yaspeller": "^2.6.0"
  },
  "engines": {
    "node": ">=0.10"
  },
  "license": "MIT",
  "main": "lib/cloudcmd.js"
}
